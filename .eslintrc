{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "env": {
    "browser": true
  },
  "globals": {
    "DEVELOPMENT": false,
    "PRODUCTION": false,
    "__DEBUG__": false
  },
  "rules": {
    "no-console": 2,
    "block-spacing": [2, "always"],
    "new-parens": 2,
    "no-dupe-class-members": 2,
    "no-this-before-super": 1,
    "prefer-template": 2
  }
}
